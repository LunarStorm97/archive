      - name: Split large file
        if: ${{ env.file_size >= 2147483648 }}
        working-directory: /mnt/workspace
        run: |
          split -b 1G ${{ env.file_name }} ${{ env.file_name }}_part_
          rm ${{ env.file_name }}

      - name: GH Release
        uses: softprops/action-gh-release@v2.2.1
        with:
          files: /mnt/workspace/*
          name: ${{ env.file_name }}
          tag_name: ${{ env.file_name }}Today's build on the road lolhttps://software.download.prss.microsoft.com/dbazure/Win8.1_SingleLang_EnglishInternational_x32.iso?t=e2f78f28-5880-44a4-a660-9eb13cc155b3&P1=1740059046&P2=601&P3=2&P4=PPpSS54rGEysjbJpS8ba8J5piy12TMsRkbF42PqN0aSFOF%2b1eSA9bCG3wsyuH8tEWFQD%2fQBw5wySahlhhqvhpeE94fSq91VBfWHUPni%2bRHRCnnhtbAWkWDSkvZ3FF0LkX%2fZ8Ys3lrlGlb43ymnP61Qmud6bWQkUaqeWS5j3EyWgMZvj95ZyaBfB%2faifAl80%2fgD4A7vaoX3kDOMxUumyzPTMf4B0qsEPkubhZk9Flas%2bc%2bmSyv53xJjaAp35eMGzVcMHSDpfvlV1epsuomtAfBlHtcmJxr%2bqZSEceKVn8krzfrTz1Wkoa09bKEKtpvC%2b4N96c1m%2fAAhZ%2fbyus1EOGrw%3d%3dname: Upload to Release

on:
  workflow_dispatch:
    inputs:
      download_url:
        description: Download file URL
        required: true
        default: http://storage.googleapis.com/google-code-archive-downloads/v2/code.google.com/jquery-speedtest/100MB.txt

jobs:
  release:
    runs-on: ubuntu-latest
    steps:
      - name: Prepare environment
        run: |
          sudo mkdir -p /mnt/workspace
          sudo chown $USER:$USER /mnt/workspace

      - name: Download file
        working-directory: /mnt/workspace
        run: |
          wget --content-disposition "${{ github.event.inputs.download_url }}"
          file_name=$(ls)
          echo file_name=$file_name >> $GITHUB_ENV

      - name: Check file size
        working-directory: /mnt/workspace
        run: |
          file_size=$(stat -c%s ${{ env.file_name }})
          echo file_size=$file_size >> $GITHUB_ENV

      - name: Split large file
